@{
  Layout = "_Layout";
}

@using LocalEats.Models
@model LocalEats.Models.Cuisine


<nav>
  <p>@Html.ActionLink("Back to details", "Details", new {id = Model.CuisineId}) | @Html.ActionLink("Delete Cuisine", "Delete", new {id = Model.CuisineId})</p>
</nav>

@using(Html.BeginForm())
{
  @Html.HiddenFor(model => model.CuisineId)
  @Html.LabelFor(cuisine => cuisine.CuisineType)
  @Html.TextBoxFor(cuisine => cuisine.CuisineType)

  <input type="submit" value="Make Edit">
}
