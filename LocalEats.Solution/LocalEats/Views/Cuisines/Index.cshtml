@{
  Layout = "_Layout";
}

@using LocalEats.Models
@model List<LocalEats.Models.Cuisine>



@if (@Model.Count == 0)
{
  <p>...no cuisines added yet</p>
}

<button>@Html.ActionLink("Add Cuisine", "Create")</button>

@foreach(Cuisine cuisine in Model)
{
  <li>@Html.ActionLink($"{@cuisine.CuisineType}", "Details", new {id = @cuisine.CuisineId})</li>;
}
